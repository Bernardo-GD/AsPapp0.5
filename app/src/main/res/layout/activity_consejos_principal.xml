<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConsejosPrincipal">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:contentDescription="@string/descripcionImagenes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tb_consejos_1" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCalendario"
        app:layout_constraintStart_toEndOf="@+id/imageView27"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:srcCompat="@drawable/icono_menu_calendario" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irAmorometros"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:srcCompat="@drawable/icono_menu_amorometros" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:contentDescription="@string/descripcionImagenes"
        app:layout_constraintStart_toEndOf="@+id/imageView28"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:srcCompat="@drawable/s_icono_menu_consejos" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:contentDescription="@string/descripcionImagenes"
        app:layout_constraintStart_toEndOf="@+id/imageView29"
        app:layout_constraintTop_toBottomOf="@+id/imageView18"
        app:srcCompat="@drawable/icono_menu_dinamicas" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCategoria1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        app:srcCompat="@drawable/consejo1" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="4dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCategoria2"
        app:layout_constraintStart_toEndOf="@+id/imageView35"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        app:srcCompat="@drawable/consejo2" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="68dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCategoria3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView35"
        app:srcCompat="@drawable/consejo3" />

    <ImageView
        android:id="@+id/imageView38"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="52dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCategoria4"
        app:layout_constraintStart_toEndOf="@+id/imageView37"
        app:layout_constraintTop_toBottomOf="@+id/imageView36"
        app:srcCompat="@drawable/consejo4" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="36dp"
        android:contentDescription="@string/descripcionImagenes"
        android:onClick="irCategoria5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView38"
        app:srcCompat="@drawable/consejo5" />

</androidx.constraintlayout.widget.ConstraintLayout>